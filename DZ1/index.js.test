import { nums } from "./index.js";

it("calls console.log with 7+2 and 7*2", () => {
  let consoleSpy = jest.spyOn(console, "log");
  nums(7, 2);
  expect(consoleSpy).toHaveBeenCalledWith(9, 14);
});

it("calls console.log with -1+0 and -1*0", () => {
  let consoleSpy = jest.spyOn(console, "log");
  nums(-1, 0);
  expect(consoleSpy).toHaveBeenCalledWith(-1, -0);
});

it("calls console.log with NaN", () => {
  let consoleSpy = jest.spyOn(console, "log");
  nums();
  expect(consoleSpy).toHaveBeenCalledWith("Error");
});

it("calls console.log with String and Null", () => {
  let consoleSpy = jest.spyOn(console, "log");
  nums("String", null);
  expect(consoleSpy).toHaveBeenCalledWith("Error");
});
